<?xml version="1.0" encoding="UTF-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#323232" />
      <stroke
          android:width="1dp"
          android:color="#e1e1e1" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
      <size 
      	  android:width="250dp" />
    </shape>
  </item>
  <item android:state_enabled="false">
    <shape>
      <solid
          android:color="#323232" />
      <stroke
          android:width="1dp"
          android:color="#404040" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
      <size 
      	  android:width="250dp" />
    </shape>
  </item>
  <item>
    <shape>
      <solid
          android:color="#323232" />
      <stroke
          android:width="1dp"
          android:color="#626262" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
      <size 
      	  android:width="250dp" />
    </shape>
  </item>
</selector>

